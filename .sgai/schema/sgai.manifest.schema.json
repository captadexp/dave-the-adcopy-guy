{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "contributors": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "link": {
                        "type": "string"
                    },
                    "image": {
                        "type": "string"
                    }
                }
            }
        },
        "domains": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}
